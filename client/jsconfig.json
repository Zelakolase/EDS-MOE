{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@Context/*": ["context/*"],
			"@Context": ["context"],
			"@Components/*": ["components/*"],
			"@Components": ["components"],
			"@Pages/*": ["pages/*"],
			"@Pages": ["pages"],
			"@Layout/*": ["layout/*"],
			"@Layout": ["layout"],
			"@Redux/*": ["redux/*"],
			"@Redux": ["redux"],
			"@Util/*": ["util/*"],
			"@Util": ["util"],
			"@Assests": ["assests"],
			"@Assests/": ["assests/"],
			"@CONSTANTS": ["constants"],
			"@Theme": ["theme.js"],
			"@API": ["api"],
			"@API/": ["api/*"],
			"@Auth": ["auth"],
			"@Hooks/": ["hooks/*"],
			"@Hooks": ["hooks"]
		}
	}
}
